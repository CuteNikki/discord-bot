  "config-level": {
    "name": "level-configuration",
    "description": "Configure the level module",
    "options": [
      {
        "name": "show",
        "description": "Shows the current configuration",
        "options": [
          {
            "name": "all",
            "description": "Shows the entire configuration"
          },
          {
            "name": "state",
            "description": "Shows the state of the level module"
          },
          {
            "name": "announcement",
            "description": "Shows the announcement configuration"
          },
          {
            "name": "rewards",
            "description": "Shows the reward roles and their required level"
          },
          {
            "name": "ignored-roles",
            "description": "Shows the roles that won't be gaining XP"
          },
          {
            "name": "ignored-channels",
            "description": "Shows the channels that users won't be gaining XP in"
          },
          {
            "name": "enabled-channels",
            "description": "If enabled channels are set, users will only be gaining XP in those"
          }
        ]
      },
      {
        "name": "toggle",
        "description": "Toggles the level module",
        "options": [
          {
            "name": "on",
            "description": "Turns the level module on"
          },
          {
            "name": "off",
            "description": "Turns the level module off"
          }
        ]
      },
      {
        "name": "announcement",
        "description": "Configures where the level up is announced",
        "options": [
          {
            "name": "user-channel",
            "description": "Level ups will be announced in the user's channel and deleted within 5 seconds"
          },
          {
            "name": "other-channel",
            "description": "Level ups will be announced in a specified channel",
            "options": [
              {
                "name": "channel",
                "description": "The channel to send the level up announcement in"
              }
            ]
          },
          {
            "name": "direct-message",
            "description": "Level ups will be announced in direct messages with the user"
          },
          {
            "name": "none",
            "description": "Level ups will not be announced"
          }
        ]
      },
      {
        "name": "rewards",
        "description": "Configure the level up rewards",
        "options": [
          {
            "name": "add",
            "description": "Adds a level up reward",
            "options": [
              {
                "name": "role",
                "description": "The role to give at level"
              },
              {
                "name": "level",
                "description": "The level required to get the role"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes a level up reward",
            "options": [
              {
                "name": "role-id",
                "description": "The id of role to remove"
              }
            ]
          }
        ]
      },
      {
        "name": "ignored-roles",
        "description": "Configure the ignored roles",
        "options": [
          {
            "name": "add",
            "description": "Adds an ignored role",
            "options": [
              {
                "name": "role",
                "description": "The role to ignore"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes an ignored role",
            "options": [
              {
                "name": "role-id",
                "description": "The id of role to remove"
              }
            ]
          }
        ]
      },
      {
        "name": "ignored-channels",
        "description": "Configure the ignored channels",
        "options": [
          {
            "name": "add",
            "description": "Adds an ignored channel",
            "options": [
              {
                "name": "channel",
                "description": "The channel to ignore"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes an ignored channel",
            "options": [
              {
                "name": "channel-id",
                "description": "The id of channel to remove"
              }
            ]
          }
        ]
      },
      {
        "name": "enabled-channels",
        "description": "Configure the enabled channels",
        "options": [
          {
            "name": "add",
            "description": "Adds an enabled channel",
            "options": [
              {
                "name": "channel",
                "description": "The channel to add"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes an enabled channel",
            "options": [
              {
                "name": "channel-id",
                "description": "The id of channel to remove"
              }
            ]
          }
        ]
      },
      {
        "name": "xp",
        "description": "Manage the XP of a user",
        "options": [
          {
            "name": "add",
            "description": "Adds XP to a user",
            "options": [
              {
                "name": "user",
                "description": "The user to add XP to"
              },
              {
                "name": "xp",
                "description": "The XP to add to user"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes XP from a user",
            "options": [
              {
                "name": "user",
                "description": "The user to remove XP from"
              },
              {
                "name": "xp",
                "description": "The XP to remove from user"
              }
            ]
          },
          {
            "name": "set",
            "description": "Sets XP of a user",
            "options": [
              {
                "name": "user",
                "description": "The user to set XP of"
              },
              {
                "name": "xp",
                "description": "The XP to set the user to"
              }
            ]
          }
        ]
      },
      {
        "name": "level",
        "description": "Manage the level of a user",
        "options": [
          {
            "name": "add",
            "description": "Adds level to a user",
            "options": [
              {
                "name": "user",
                "description": "The user to add level to"
              },
              {
                "name": "level",
                "description": "The level to add to user"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes level from a user",
            "options": [
              {
                "name": "user",
                "description": "The user to remove level from"
              },
              {
                "name": "level",
                "description": "The level to remove from user"
              }
            ]
          },
          {
            "name": "set",
            "description": "Sets level of a user",
            "options": [
              {
                "name": "user",
                "description": "The user to set level of"
              },
              {
                "name": "level",
                "description": "The level to set the user to"
              }
            ]
          }
        ]
      }
    ]
  },

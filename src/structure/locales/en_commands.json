{
  "config-developer": {
    "name": "developer-configuration",
    "description": "Only for staff/developers of the bot",
    "options": [
      {
        "name": "support-guild-id",
        "description": "Configures the support guild id",
        "options": [
          {
            "name": "set",
            "description": "Sets the support guild id",
            "options": [
              {
                "name": "guild-id",
                "description": "The support guild id"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes the support guild id"
          },
          {
            "name": "show",
            "description": "Shows the support guild id"
          }
        ]
      },
      {
        "name": "support-invite-url",
        "description": "Configures the support invite url",
        "options": [
          {
            "name": "set",
            "description": "Sets the support invite url",
            "options": [
              {
                "name": "invite-url",
                "description": "The support invite url"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes the support invite url"
          },
          {
            "name": "show",
            "description": "Shows the support invite url"
          }
        ]
      },
      {
        "name": "bot-invite-url",
        "description": "Configures the bot invite url",
        "options": [
          {
            "name": "set",
            "description": "Sets the bot invite url",
            "options": [
              {
                "name": "invite-url",
                "description": "The bot invite url"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes the bot invite url"
          },
          {
            "name": "show",
            "description": "Shows the bot invite url"
          }
        ]
      },
      {
        "name": "badges",
        "description": "Configures the badges of a user",
        "options": [
          {
            "name": "add",
            "description": "Adds a badge to a user",
            "options": [
              {
                "name": "user",
                "description": "The user to add a badge to"
              },
              {
                "name": "badge",
                "description": "The badge to add",
                "choices": ["Translator", "Bughunter", "Expert Bughunter", "Supporter", "Staff Member", "Developer"]
              }
            ]
          },
          {
            "name": "remove",
            "description": "Removes a badge from a user",
            "options": [
              {
                "name": "user",
                "description": "The user to remove a badge from"
              },
              {
                "name": "badge",
                "description": "The badge to remove",
                "choices": ["Translator", "Bughunter", "Expert Bughunter", "Supporter", "Staff Member", "Developer"]
              }
            ]
          },
          {
            "name": "show",
            "description": "Lists all badges of a user",
            "options": [
              {
                "name": "user",
                "description": "The user to view badges of"
              }
            ]
          }
        ]
      },
      {
        "name": "bans",
        "description": "Configures the banned users",
        "options": [
          {
            "name": "add",
            "description": "Bans a user from the bot",
            "options": [
              {
                "name": "user",
                "description": "The user to ban"
              }
            ]
          },
          {
            "name": "remove",
            "description": "Unban a user from the bot",
            "options": [
              {
                "name": "user",
                "description": "The user to unban"
              }
            ]
          },
          {
            "name": "show",
            "description": "Lists all banned users"
          }
        ]
      }
    ]
  },
